def linear_search(a,x,k):
   
       for i in range(len(a)):
            if(a[i] == x):
               return i
       return -1
 x=int(input("enter the list of elements"))
 a=[]
 for i in range(x):
      y=int((input("ele")))
      a.append(y)
 k=int(input("enter a value to be serached"))
